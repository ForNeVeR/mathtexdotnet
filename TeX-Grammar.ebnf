expr = term , [ ( "+" | "-" ) , epxr ] ;
term = indexed value , [ ( "*" | "/" ) , term ] ;
indexed value = value , ( [ "^" , index ] , [ "_" , index ] ) | ( [ "_" , index ] , [ "^" , index ] ) ;
index = raw value | group ;
group = "{" , expr , "}" ;
value = [ "+" | "-" ] , ( raw value | group | bracket expr ) ;
raw value = ? number ? | ? symbol ? ;
bracket expr = "{" , expr , "}" | "(" , expr , ")" | "[" , expr , "]" | "\{" , expr , "\}" ;
